{
  "name": "@easynet/agent-tool-hub",
  "version": "1.0.9",
  "description": "Agent Tool Hub: multi-protocol tool registry, PTC runtime, and adapter layer for MCP/LangChain/n8n/SKILL",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agent-tool-hub": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "example:agent-toolhub": "node examples/agent-toolhub-example.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dump:tools": "npm run build && node scripts/dump-tools.mjs",
    "lint:lines": "./scripts/check-line-count.sh",
    "lint:functions": "python3 scripts/check-function-length.py"
  },
  "dependencies": {
    "@easynet/n8n-local": "^0.0.37",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cockatiel": "^3.2.1",
    "eventemitter3": "^5.0.1",
    "jmespath": "^0.16.0",
    "js-yaml": "^4.1.1",
    "mustache": "^4.2.0",
    "p-retry": "^6.2.1",
    "p-timeout": "^6.1.4",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jmespath": "^0.15.2",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "semantic-release": "^24.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.0",
    "@modelcontextprotocol/sdk": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@modelcontextprotocol/sdk": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
